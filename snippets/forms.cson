'.text.html':
  'Form Horizontal':
    'prefix': ''
    'body': """
      <form ${1:action="$2"} method="${3:POST}" class="form-horizontal" role="form">
		    <div class="form-group">
			   <legend>${4:Form title}</legend>
		    </div>
		    ${6:Body}
		    <div class="form-group">
			    <div class="col-sm-10 col-sm-offset-2">
				   <button type="submit" class="btn btn-primary">${5:Submit}</button>
			    </div>
		    </div>
      </form>
    """
